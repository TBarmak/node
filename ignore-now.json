{
  "version": 2,
  "name": "markdown-zeit",
  "builds": [
    {
      "src": "*.css",
      "use": "@now/static"
    },
    {
      "src":"**/*.csv",
      "use":"@now/static"
    },
    {
      "src":"**/*.ipynb",
      "use":"@now/static"
    }
  ],
  "routes":[{
      "src": "/*",
      "dest": "/*",
      "methods": ["GET"]
    }]
}
