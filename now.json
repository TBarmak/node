{
  "version": 2,
  "name": "markdown-zeit",
  "builds": [
    {
      "src": "*.css",
      "use": "@now/static"
    },
    {
      "src": "**/*.md",
      "use": "@now/md",
      "config": {
        "title": "Node Fall 2020",
        "language": "en",
        "meta": [{ "name": "description", "content": "Mirror to Node repository"}],
        "css": "/style.css"
      }
    },
    {
      "src":"**/*.csv",
      "use":"@now/static"
    }
    {
      "src":"**/*.ipynb",
      "use":"@now/static"
    }
    {
      "src":"index.html",
      "use":"@now/static"
    }
  ],
  "routes":[{
      "src": "/*",
      "dest": "/*",
      "methods": ["GET"]
    }]
}
